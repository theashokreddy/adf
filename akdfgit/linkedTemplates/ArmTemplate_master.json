{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "akdfgit"
		},
		"adlsLoans_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adlsLoans'"
		},
		"loansAdls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'loansAdls'"
		},
		"loansBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'loansBlob'"
		},
		"adlsLoans_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://bankadls.dfs.core.windows.net/"
		},
		"loansAdls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://bankadls.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "akdfgit_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adlsLoans_accountKey": {
						"value": "[parameters('adlsLoans_accountKey')]"
					},
					"loansAdls_accountKey": {
						"value": "[parameters('loansAdls_accountKey')]"
					},
					"loansBlob_connectionString": {
						"value": "[parameters('loansBlob_connectionString')]"
					},
					"adlsLoans_properties_typeProperties_url": {
						"value": "[parameters('adlsLoans_properties_typeProperties_url')]"
					},
					"loansAdls_properties_typeProperties_url": {
						"value": "[parameters('loansAdls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}